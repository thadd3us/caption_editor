{
  "name": "vtt-editor",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:coverage": "playwright test --reporter=html,json",
    "test:coverage": "npm run test:unit:coverage && playwright test --reporter=html && npx nyc report --reporter=text --reporter=html"
  },
  "dependencies": {
    "ag-grid-community": "^31.1.1",
    "ag-grid-vue3": "^31.1.1",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^27.0.1",
    "nyc": "^17.1.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-istanbul": "^7.2.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.6"
  }
}
